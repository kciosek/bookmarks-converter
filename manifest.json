{
  "name": "Bookmarks Import",
  "version": "0.1",
  "description": "Import bookmarks from html file generated by getpocket.com",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Bookmarks Import",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
